SELECT
  FROM /scmtms/d_trqrot AS trqrot
  JOIN /scmtms/d_trqitm AS trqitm ON trqitm~parent_key = trqrot~db_key
  JOIN /scmtms/d_trqpty AS trqpty ON trqpty~parent_key = trqrot~db_key
  FIELDS
   trqrot~db_key,
   trqrot~trq_id,
   trqrot~trq_type,
   trqrot~order_date,
   trqrot~shipper_id,
   trqrot~shipper_key,
   trqrot~consignee_id,
   trqrot~consignee_key,
   trqrot~order_party_id,
   trqrot~order_party_key,
   trqrot~src_loc_id,
   trqrot~src_loc_key,
   trqrot~des_loc_id,
   trqrot~des_loc_key,
   trqrot~base_btd_id,
   trqrot~sales_org_id,
   trqrot~mot_cat,
   trqrot~shipping_type,
   trqrot~traffic_direct,
   trqrot~movem_type,
   trqitm~db_key AS trqitm_key,
   trqitm~parent_key,
   trqitm~item_parent_key,
   trqitm~item_cat,
   trqitm~main_cargo_item,
   trqitm~gro_wei_val,
   trqitm~gro_wei_uni,
   trqitm~gro_vol_val,
   trqitm~gro_vol_uni,
   trqitm~net_wei_val,
   trqitm~net_wei_uni,
   trqitm~qua_pcs_val,
   trqitm~qua_pcs_uni,
   trqitm~dgo_indicator,
   trqitm~tures_tco,
   trqitm~tures_cat,
   trqitm~item_type,
   trqpty~db_key AS trqpty_key,
   trqpty~party_rco,
   trqpty~party_id,
   trqpty~party_uuid
  INTO TABLE @DATA(lt_trq_ifno).
